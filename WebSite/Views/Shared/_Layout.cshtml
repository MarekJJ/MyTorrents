@*@using WebSite.Infrastructure.Helpers*@

<!DOCTYPE html>
<html>
<head>


    <!-- Bootstrap -->
    <link href="/Temp1/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="/Temp1/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="/Temp1/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- jQuery custom content scroller -->
    <link href="/Temp1/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet" /> <!--tego nie mam w vendor-->
    <!-- Custom Theme Style -->
    <link href="/Temp1/build/css/custom.min.css" rel="stylesheet">  <!--kustomowe css-->




    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
