                    <div class="right_col" role="main">
                        <div class="">
                            <div class="page-title">
                                <div class="title_left">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>








                        @*---------------------------------------------*@
                        <!-- jQuery -->
                        <script src="/Temp1/vendors/jquery/dist/jquery.min.js"></script> <!--jest jquery-->
