                        <script src="/Temp1vendors/nprogress/nprogress.js"></script>
                        <!-- jQuery custom content scroller -->
                        <script src="/Temp1vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>

                        <!-- Custom Theme Scripts -->
                        <script src="/Temp1/build/js/custom.min.js"></script>

                    </div>
                </div>
            </div>
        </div>
